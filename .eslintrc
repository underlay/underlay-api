{ 
    parser: "babel-eslint",
    extends: "eslint-config-airbnb",
    env: {
        "browser": true,
        "node": true,
        "mocha": true
    },
    plugins: [
        "react", "import"
    ],
    "rules": {
        "arrow-spacing": 0,
        "arrow-body-style": 0,
        "comma-dangle": 0,
        "func-names": 0,
        "import/no-extraneous-dependencies": 0,
        "indent": [2, "tab", { "MemberExpression": 0 }],
        "object-shorthand": 0,
        "no-console": 0,
        "no-tabs": 0,
        "no-underscore-dangle": 0,
        "space-before-function-paren": 0,
    },
}
